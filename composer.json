{
  "name": "gotryiton/gotron",
  "description": "Gotron PHP Framework",
  "keywords": ["framework"],
  "homepage": "https://github.com/gotryiton/gotron",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gotryiton/pheanstalk"
    },
    {
      "type": "package",
      "package": {
        "name": "spyc/spyc",
        "version": "0.5",
        "dist": {
          "url": "http://spyc.googlecode.com/files/spyc-0.5.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "rmccue/requests": ">=1.0",
    "swiftmailer/swiftmailer": "4.1.x",
    "pda/pheanstalk": "dev-master",
    "spyc/spyc": ">=0.5",
    "Aura/Autoload": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "Gotron": "lib/",
      "ActiveRecord": "lib/",
      "Requests": "library/",
      "Swift": "lib/classes/",
      "Spyc": "spyc/",
      "Aura": "src/"
    }
  }
}